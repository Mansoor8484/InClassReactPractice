<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
         prefHeight="40.0" prefWidth="1200.0" spacing="32.0" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.HBox">
    <Button mnemonicParsing="false" style="-fx-background-color: #DFF0D8;" text="Overview" textFill="#3b3b3b" onMouseClicked="#onOverviewClick">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </Button>
    <Button mnemonicParsing="false" style="-fx-background-color: #DFF0D8;"
            text="Transactions" textFill="#3b3b3b" onMouseClicked="#onTransactionsClick">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </Button>
    <Button mnemonicParsing="false" style="-fx-background-color: #DFF0D8;"
            text="Budgeting" textFill="#3b3b3b" onMouseClicked="#onBudgetingClick">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </Button>
    <Button mnemonicParsing="false" style="-fx-background-color: #DFF0D8;"
            text="Security" textFill="#3b3b3b" onMouseClicked="#onSecurityClick">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
    </Button>
</fx:root>
