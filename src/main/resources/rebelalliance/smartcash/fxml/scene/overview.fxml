<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="rebelalliance.smartcash.controller.scene.OverviewController">
    <VBox fx:id="accountsBox" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="300.0" />
    <Button layoutX="339.0" layoutY="342.0" mnemonicParsing="false" onAction="#testGoToTransactions" rotate="9.5"
            text="Go to Transactions" />
    <PieChart fx:id="pieChart" labelLineLength="0" layoutX="314.0" layoutY="14.0" prefHeight="273.0" prefWidth="369.0"
              title="Account Make Up" />
    <LineChart fx:id="accountsWeekOverWeek" layoutX="689.0" layoutY="10.0" prefHeight="400.0" prefWidth="497.0"
               title="Accounts Over Time">
        <xAxis>
            <NumberAxis fx:id="lineChartXAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
            <NumberAxis side="LEFT" />
        </yAxis>
    </LineChart>
</AnchorPane>
